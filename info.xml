<?xml version="1.0"?>
<extension key="mayfirst" type="module">
  <file>mayfirst</file>
  <name>May First Membership custom code.</name>
  <description>Provides membership sign up and renewal code.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Jamie McClelland</author>
    <email>jamie@mayfirst.coop</email>
  </maintainer>
  <urls>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2021-01-18</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Mayfirst</namespace>
    <format>22.12.1</format>
  </civix>
  <requires>
    <ext>com.drastikbydesign.stripe</ext>
  </requires>
  <mixins>
    <mixin>ang-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Mayfirst_Upgrader</upgrader>
</extension>
